{
	"name": "Node.js & MariaDB",
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",

	"forwardPorts": [3000, 3306],

	// Example: Run npm install automatically
	"postCreateCommand": "npm install",

	// Uncomment if you want to customize VS Code settings/extensions here
	"customizations": {
  	"vscode": {
    	"extensions": [
    	  "GitHub.copilot",
    	  "GitHub.copilot-chat"
    	]
  		}
	}

	// Uncomment if you prefer root user inside the dev container.
	// "remoteUser": "root"
}
